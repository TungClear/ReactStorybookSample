    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.10",
    "babel-loader": "^8.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
